{
	"env": {
        "test_client": {
            "plugins": [
                "./tools/babel-plugin-preact-to-react.js"
            ],
            "presets": ["react"]
        },
        "test_server": {
            "plugins": [
                "rewire"
            ]
        },

        "webpack": {
            "presets": ["react", "latest"],
            "plugins": [
                ["transform-react-jsx", { "pragma": "h" }]
            ]
        }
    },
    "presets": [
        [
            "latest", {
                "es2015": false
            }
        ]
    ],
    "plugins": ["transform-es2015-modules-commonjs"]
}
